<h1>ログインページ</h1>

<%= form_with(url: login_path, scope: :session, local: true) do |f| %>

	<%= f.label :email, "メールアドレス" %>
	<%= f.email_field :email %>
	
	<%= f.label :password, "パスワード" %>
	<%= f.password_field :password %>
	
	<%= f.submit "ログイン" %>
<%end%>

<p>新規作成<%= link_to "ユーザの新規作成", signup_path %></p>